{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pages/*": [
        "src/pages/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@Login/*": [
        "src/Pages/Login/*"
      ],
      "@Pages/*": [
        "src/Pages/*"
      ],
      "@D/*": [
        "src/Pages/Login/D/*"
      ],
      "@Dashboard/*": [
        "src/Pages/Dashboard/*"
      ],
      "@Components/*": [
        "src/Components/*"
      ],
      "@Lib/*": [
        "src/Lib/*"
      ],
      "@Test/*": [
        "src/Pages/Test/*"
      ]
    }
  }
}